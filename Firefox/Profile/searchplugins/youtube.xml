<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>YouTube</os:ShortName>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/png;base64,</os:Image>
<SearchForm>http://www.youtube.com</SearchForm>
<os:Url type="text/html" method="GET" template="http://www.youtube.com/results" resultDomain="youtube.com">
  <os:Param name="search_Type" value=""/>
  <os:Param name="search_query" value="{searchTerms}"/>
  <os:Param name="aq" value="f"/>
</os:Url>
</SearchPlugin>