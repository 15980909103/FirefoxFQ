<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Digg</os:ShortName>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/png;base64,</os:Image>
<SearchForm>http://digg.com/</SearchForm>
<os:Url type="text/html" method="GET" template="http://digg.com/search" resultDomain="digg.com">
  <os:Param name="q" value="{searchTerms}"/>
  <os:Param name="submit" value=""/>
</os:Url>
</SearchPlugin>